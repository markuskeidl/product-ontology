                skos:example "Crafter"@en,
                    "Ducato"@en,
                    "Jumpy"@en,
                    "Sprinter"@en,
                    "Transit"@en ;
                skos:prefLabel "Kleintransporter"@de,
                    "Van"@en .
            
            cx-taxo:InternalCombustionEngineVehicle a skos:Concept ;
