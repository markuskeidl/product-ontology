                                cx-taxo:FourAxleSemiTrailerTruck a skos:Concept ;
                                    skos:broader cx-taxo:Truck ;
                                    skos:prefLabel "Four-Axle Semi-Trailer Truck"@en .
                                
                                cx-taxo:GarbageTruck a skos:Concept ;
